<% if  @clients.present? %>
<table class="table">
	<thead>
    <tr>
  		<th>#</th>
  		<th>Email</th>
      <th>Name</th>
      <th colspan="5"></th>
  		<th>Action</th>
    </tr>
	</thead>
	 <tbody class="active_listings">
    <%= render partial: "client_list", locals: {clients: @clients} %>
  </tbody>
</table>
<% else %>
  <p align="center">No Clients</p>  
<% end %>