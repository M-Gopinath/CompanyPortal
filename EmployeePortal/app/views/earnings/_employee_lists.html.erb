<section class="content">
  <div class="panel">
    <div class="row">
       <div class="content-header">
        <p class="text-center back-pre-btn income-title">
          <span class="pull-left"><%= link_to "<<", employee_payslips_path(date: date - 1.year),class: "btn btn-default", remote: true %></span>
          <span><b><%= date.strftime("%Y") %></b></span>
          <span class="pull-right"><%= link_to ">>", employee_payslips_path(date: date + 1.year),class: "btn btn-default", remote: true %></span>
        </p>
      </div>
      <div class="header-border"></div>
    </div>
    <div class="table-responsive">
      <table class="table">
        <thead>
          <tr>
            <th>Emb.id</th>
            <th>Email</th>
            <th colspan="3" class="text-center">Action
            </th>
            <th></th>
          </tr>
        </thead>
        <tbody class="active_listings">
          <%= render partial: "user_listings", locals: {actived_employees: actived_employees, date: date} %>
        </tbody>
      </table>
    </div>
    <div class="pagination-content">
      <%= will_paginate actived_employees, renderer: BootstrapPagination::Rails %>
    </div>
  </div>
</section>