$('.prefill_timesheet_<%= @day %>').html("<%= escape_javascript(render partial: 'time_sheet_form', locals: {task: @task, tasks: @tasks, day: @day}) %>");
$(".full_calendar").datepicker({startDate: '<%= @task.try(:start_date).present? ? @task.try(:start_date).strftime("%d-%m-%Y") : "" %>',format: 'dd-mm-yyyy'});
$('.timepicker').timepicker({ 'scrollDefault': 'now' });